{
  "compilerOptions": {
    "types": ["jest", "cypress"],
    "resolveJsonModule": true,
    "target": "es6",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "cypress/**/*.ts", "cypress/**/*.js"]
}
